Public Function 差異抽出() As Boolean
    Dim 残在庫 As Double
    Dim 比較元一覧 As New cls_DBマネージャー
    Dim 比較先一覧 As New cls_DBマネージャー
    Dim 比較先 As New cls_DBマネージャー
    Dim 比較元 As New cls_DBマネージャー
    Dim 比較元だけにある As New cls_DBマネージャー
    Dim 比較先だけにある As New cls_DBマネージャー
    Dim 差異あり比較元 As New cls_DBマネージャー
    Dim 差異あり比較先 As New cls_DBマネージャー
    Dim i As Integer
    Dim k As Integer
    Dim j As Integer
    
    '初期化
'    Worksheets(\"比較先だけにある\").Cells.Select
'    Selection.ClearContents
'
'    Worksheets(\"比較先だけにある\").Cells.Select
'    Selection.ClearContents
'
'    Worksheets(\"差異あり\").Cells.Select
'    Selection.ClearContents
    
    Call 比較元シート一覧データ取得(比較元一覧)
    k = 1
    l = 1
    i = 2
    j = 2
    Do Until 比較元一覧.rs.EOF = True
        Call 比較先シート一覧データを注文番号で取得(比較先, 比較元一覧.rs!受注番号)
        '見出し行を書き出し
        For k = 1 To 比較元一覧.rs.Fields.Count
            Worksheets(\"比較元だけにある\").Cells(1, k) = 比較元一覧.rs.Fields(k - 1).Name
        Next
        
        For k = 1 To 比較元一覧.rs.Fields.Count
            Worksheets(\"差異あり\").Cells(1, k) = 比較元一覧.rs.Fields(k - 1).Name
        Next
        k = k + 1
        For l = 1 To 比較先.rs.Fields.Count
            Worksheets(\"差異あり\").Cells(1, k) = 比較先.rs.Fields(l - 1).Name
            k = k + 1
        Next
        If 比較先.rs.RecordCount = 0 Then
            Debug.Print \"比較元:\" & 比較元一覧.rs!受注番号
            Call 比較元シート一覧データを注文番号で取得(比較元だけにある, 比較元一覧.rs!受注番号)

            renge = \"A\" & i
            Worksheets(\"比較元だけにある\").range(renge).CopyFromRecordset 比較元だけにある.rs
            i = i + 1
        Else
            diff = 比較元一覧.rs!売上合計 - 比較先.rs!商品売上
            If diff > 0 Then
                Debug.Print \"差異あり:\" & 比較元一覧.rs!受注番号
                Call 比較元シート一覧データを注文番号で取得(差異あり比較元, 比較元一覧.rs!受注番号)
                renge1 = \"A\" & j
                Worksheets(\"差異あり\").range(renge1).CopyFromRecordset 差異あり比較元.rs
                Call 比較先シート一覧データを注文番号で取得(差異あり比較先, 比較元一覧.rs!受注番号)
                renge2 = \"T\" & j
                Worksheets(\"差異あり\").range(renge2).CopyFromRecordset 差異あり比較先.rs
                j = j + 1
            End If
        End If
        
        比較元一覧.rs.MoveNext
    Loop
    
    Call 比較先シート一覧データ取得(比較先一覧)
    k = 1
    i = 2
     Do Until 比較先一覧.rs.EOF = True
        Call 比較元シート一覧データを注文番号で取得(比較元, 比較先一覧.rs!注文番号)
        '見出し行を書き出し
        For k = 1 To 比較先一覧.rs.Fields.Count
            Worksheets(\"比較先だけにある\").Cells(1, k) = 比較先一覧.rs.Fields(k - 1).Name
        Next
        If 比較元.rs.RecordCount = 0 Then
            Debug.Print \"比較先:\" & 比較先一覧.rs!注文番号
            Call 比較先シート一覧データを注文番号で取得(比較先だけにある, 比較先一覧.rs!注文番号)
            renge = \"A\" & i
            Worksheets(\"比較先だけにある\").range(renge).CopyFromRecordset 比較先だけにある.rs
            i = i + 1
        End If
        比較先一覧.rs.MoveNext
    Loop
End Function

Private Sub 比較元シート一覧データ取得(比較元一覧 As cls_DBマネージャー)
    Dim sql As String
    sql = \"SELECT 出荷日,ECサイト,取込番号,受注番号,支払方法,売上合計,仕入合計,粗利合計,手数料,送料,利用ポイント,請求金額,売上総合計,検証用合計,出荷年年,出荷月,過不足調整金,クーポン額 FROM [比較元$]\"
    
    比較元一覧.ExecSQL (sql)
End Sub

Private Sub 比較先シート一覧データ取得(比較先一覧 As cls_DBマネージャー)
    Dim sql As String
    sql = \"SELECT * FROM [比較先$]\"
    比較先一覧.ExecSQL (sql)
End Sub

Private Sub 比較先シート一覧データを注文番号で取得(比較先 As cls_DBマネージャー, 注文番号)
    Dim sql As String
    sql = \"SELECT * FROM [比較先$] WHERE 注文番号 = \" & \"'\" & 注文番号 & \"'\"
    比較先.ExecSQL (sql)
End Sub

Private Sub 比較元シート一覧データを注文番号で取得(比較元 As cls_DBマネージャー, 注文番号)
    Dim sql As String
    sql = \"SELECT * FROM [比較元$] WHERE 受注番号 = \" & \"'\" & 注文番号 & \"'\"
    比較元.ExecSQL (sql)
End Sub
"] [object gh-gist-gist-file "gh-gist-gist-file" "cls_DBマネージャ" 2259 "https://gist.githubusercontent.com/k2works/c3231f3dde4fddd61f12/raw/e813be228c7f7335d0e3d17fe740bb94eec474f3/cls_DB%E3%83%9E%E3%83%8D%E3%83%BC%E3%82%B8%E3%83%A3" "'------------------------------------------------------------
' DBマネージャー
' 2013/03/26 K.Kakigi
'------------------------------------------------------------
Private My_Cn As Object
Private My_Rs As Object
Private My_Sql As String
 
Public Property Let sql(ByVal sql As String)
 
    My_Sql = sql
 
End Property
 
Public Property Get sql() As String
 
    sql = My_Sql
 
End Property
 
Public Property Get cn() As Object
 
    Set cn = My_Cn
 
End Property
 
 
Public Property Get rs() As Object
 
    Set rs = My_Rs
 
End Property
 
 
Private Sub Class_Initialize()
On Error GoTo Obj_Rls
    
    '変数定義
    Dim sPath As String
  
    'コネクションオブジェクト作成
    Set My_Cn = CreateObject(\"ADODB.Connection\")
    'レコードセットオブジェクト作成
    Set My_Rs = CreateObject(\"ADODB.Recordset\")
  
    'Excelパス
    sPath = ThisWorkbook.Path & \"\\\" & ThisWorkbook.Name
  
    'ファイルの存在確認
    If Dir(sPath) = \"\" Then
        MsgBox \"指定されたExcelが存在しません\"
        GoTo Obj_Rls
    End If
  
    '-------------------
    ' 対象Excelオープン
    '-------------------
    With My_Cn
        .Provider = \"MSDASQL\"
        .ConnectionString = \"Driver={Microsoft Excel Driver (*.xls)};\" & _
                            \"DBQ=\" & sPath & \"; ReadOnly=False;\" & _
                            \"Extended Properties=\"\" Excel 8.0; HDR=YES;\"\"\"
        .Open
        .CursorLocation = 3 ' クライアントサイドカーソルに変更
    End With
    
Exit Sub
    
Obj_Rls:
    'エラー確認
    If Err.Number <> 0 Then
        MsgBox Err.Description
    End If
    '開放
    Me.clear
    MsgBox \"処理完了\"
    
End Sub
 
Private Sub Class_Terminate()
 
End Sub
 
Public Sub clear()
    Set My_Cmd = Nothing
    Set My_Rs = Nothing
    Set My_Cn = Nothing
End Sub
 
Public Sub ExecSQL(sql As String)
    My_Sql = sql
    Set My_Rs = My_Cn.Execute(sql)
End Sub
 
Public Sub BeginTrans()
    cn.BeginTrans
End Sub
 
Public Sub CommitTrans()
    cn.CommitTrans
End Sub
 
Public Sub RollbackTrans()
    cn.RollbackTrans
    
    MsgBox \"以下のエラーが発生したためロールバックしました。\" & vbCrLf & _
        Err.Description, vbCritical
End Sub


